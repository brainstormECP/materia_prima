@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Backend.cshtml";
}

<div class="row">
    <div class="col-md-4 col-md-offset-4">
        <div class="portlet box blue">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-plus-square"></i>Conciliacion de Ventas
                </div>
            </div>
            <div class="portlet-body">
                <form asp-action="ExistenciasPorUeb">
                    <div class="form-group">
                        <label>Seleccione UEB</label>
                        <select name="Id" class="form-control" asp-items="@ViewBag.UebId">Seleccione UEB</select>
                    </div>
                    <div class="form-group text-center">
                        <input type="submit" class="btn btn-success" value="Generar reporte" />
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>


