@model MateriasPrimaApp.Models.Cliente
@{
    Layout = "~/Views/Shared/_Backend.cshtml";
}
<div class="row">
    <div class="col-md-4 col-md-offset-4">
        <div class="portlet box blue">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-eye"></i>Detalles Cliente
                </div>
            </div>
            <div class="portlet-body">
                <div class="row static-info">
                </div>
                <div class="row static-info">
                    <div class="col-md-6 text-center">
                        <dt>
                            @Html.DisplayNameFor(model => model.Codigo)
                        </dt>
                    </div>
                    <div class="col-md-6 text-left">
                        <dd>
                            @Html.DisplayFor(model => model.Codigo)
                        </dd>
                    </div>
                </div>
                <div class="row static-info">
                    <div class="col-md-6 text-center">
                        <dt>
                            @Html.DisplayNameFor(model => model.Nombre)
                        </dt>
                    </div>
                    <div class="col-md-6 text-left">
                        <dd>
                            @Html.DisplayFor(model => model.Nombre)
                        </dd>
                    </div>
                </div>
                <div class="row static-info">
                    <div class="col-md-6 text-center">
                        <dt>
                            @Html.DisplayNameFor(model => model.Organismo)
                        </dt>
                    </div>
                    <div class="col-md-6 text-left">
                        <dd>
                            @Html.DisplayFor(model => model.Organismo)
                        </dd>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<dl class="dl-horizontal"></dl>
<div class="text-center">
    <div class="btn-group btn-group-solid">
        <a asp-action="Index" class="btn btn-info">Atrás</a>
        <a asp-action="Edit" class="btn yellow" asp-route-id="@Model.Id">Editar</a>
    </div>
</div>
